      "value": ""
    },
    "HANDLERS": {
      "description": "prefix for commands",
      "required": false,
      "value": ""
    },
    "PACKNAME": {
      "description": "Sticker Packname",
      "required": false
    },
    "GOODBYE_MSG": {
      "description": "goodbye Message",
